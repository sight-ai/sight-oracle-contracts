{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "targetDefaults": {
    "compile": {
      "outputs": ["{projectRoot}/artifacts", "{projectRoot}/cache"],
      "cache": true
    },
    "lint:js:fix": {
      "cache": true
    },
    "lint:sol:fix": {
      "cache": true
    },
    "test": {
      "cache": true
    },
    "clean": {
      "cache": true
    }
  },
  "defaultBase": "main",
  "plugins": ["nx/plugins/package-json"]
}
